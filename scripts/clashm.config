# >>> ClashM Configuration <<< #

FISH=(
 "example.com"
)

# Customizable
MERGE="true" # Merge between BASE and PROXIES
FISHER="false"
FORCE="false" # Skip PORT & TUN Device verifier
BIN_NAME="clash"
ICON="/sdcard/Pictures/.icons/ClashM.png" # Must in SDCARD/INTERNAL Storage
DELAY="2"
ID="2022"

# Advanced
DIR="${0%/*}"
MODDIR="/data/adb/modules/REPLACE"
DATA="/data/adb/REPLACE"
CONFIG="${DATA}/config.yaml"
BASE="${DATA}/base.yaml"
PROXIES="${DATA}/proxies.yaml"

RUN="${MODDIR}/run"
PID_FILE="${RUN}/clash.pid"
RUN_FILE="${RUN}/run.log"
CORE_LOG_FILE="${RUN}/core.log"
SCRIPTS="$MODDIR/scripts"
BIN="$MODDIR/bin/$BIN_NAME"
BUSYBOX="/data/adb/magisk/busybox"


